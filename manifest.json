{
  "manifest_version": 3,
  "name": "Google Maps Easy Scrape",
  "version": "1.1",
  "author": "Mike Powers & contributors",
  "description": "Scrapes information from google maps",
  "permissions": [
    "activeTab", 
    "scripting", 
    "storage",
    "sidePanel"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "side_panel": {
    "default_path": "/pages/settings/history-sidebar-view.html"
  },
  "icons": {
    "16": "icons/gmes_16.png",
    "48": "icons/gmes_48.png",
    "128": "icons/gmes.png"
  }
}
  